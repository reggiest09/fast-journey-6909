<script>
$(document).ready(function() {
  //
  //  carouFredSel team
  $('#team .carousel.main ul').carouFredSel({
    auto: {
      timeoutDuration: 8000
    },
    responsive: true,
    prev: '.team_prev',
    next: '.team_next',
    width: '100%',
    scroll: {
      // fx : "crossfade",
      items: 1,
      duration: 1000,
      easing: "easeOutExpo"
    },
    items: {
          width: '350',
      height: 'variable', //  optionally resize item-height
      visible: {
        min: 1,
        max: 4
      }
    },
    mousewheel: false,
    swipe: {
      onMouse: true,
      onTouch: true
      }
  });
  $(window).bind("resize",updateSizes_vat).bind("load",updateSizes_vat);
  function updateSizes_vat(){
    $('#team .carousel.main ul').trigger("updateSizes");
  }
  updateSizes_vat();
}); //
$(window).load(function() {
  //
  /////// flexslider
  $('#flexslider').flexslider({
    animation: "fade",
    slideshow: true,
    slideshowSpeed: 7000,
    animationDuration: 600,
    pauseOnAction: true,
    prevText: "",
    nextText: "",
    controlNav: true,
    directionNav: true
  });
}); //
</script>

